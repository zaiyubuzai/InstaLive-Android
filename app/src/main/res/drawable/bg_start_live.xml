<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval"
            android:useLevel="true">
            <size
                android:width="96dp"
                android:height="96dp" />

            <solid android:color="@color/transparent" />
            <stroke
                android:width="1dp"
                android:color="@color/default_yellow" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/dimen_4dp"
        android:left="@dimen/dimen_4dp"
        android:right="@dimen/dimen_4dp"
        android:top="@dimen/dimen_4dp">
        <shape
            android:shape="oval"
            android:useLevel="true">
            <size
                android:width="96dp"
                android:height="96dp" />
            <solid android:color="@color/default_yellow" />
        </shape>
    </item>

</layer-list>